{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "friendsofsymfony/jsrouting-bundle": "^3.5",
        "knplabs/knp-components": "*",
        "knplabs/knp-paginator-bundle": "^6.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.*",
        "symfony/asset-mapper": "7.*",
        "symfony/console": "7.*",
        "symfony/dotenv": "7.*",
        "symfony/error-handler": "7.*",
        "symfony/expression-language": "7.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "7.*",
        "symfony/framework-bundle": "7.*",
        "symfony/http-client": "7.*",
        "symfony/http-foundation": "7.*",
        "symfony/intl": "7.*",
        "symfony/mailer": "7.*",
        "symfony/mime": "7.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "7.*",
        "symfony/process": "7.*",
        "symfony/property-access": "7.*",
        "symfony/property-info": "7.*",
        "symfony/proxy-manager-bridge": "^6.4",
        "symfony/requirements-checker": "^2.0",
        "symfony/runtime": "7.*",
        "symfony/security-bundle": "7.*",
        "symfony/serializer": "7.*",
        "symfony/stimulus-bundle": "^2.13",
        "symfony/string": "7.*",
        "symfony/translation": "7.*",
        "symfony/twig-bundle": "7.*",
        "symfony/validator": "7.*",
        "symfony/web-link": "7.*",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "7.*",
        "symfonycasts/sass-bundle": "^0.3.0",
        "twig/extra-bundle": "^3.7",
        "twig/intl-extra": "^3.7",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "dbrekelmans/bdi": "^1.1",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.*",
        "symfony/css-selector": "7.*",
        "symfony/debug-bundle": "7.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^2.1",
        "symfony/phpunit-bridge": "^6",
        "symfony/stopwatch": "7.*",
        "symfony/web-profiler-bundle": "7.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.*"
        }
    }
}
